{
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "max-classes-per-file": false,
        "max-line-length": [
            true,
            200
        ],
        "no-var-requires": false
    },
    "rulesDirectory": []
}